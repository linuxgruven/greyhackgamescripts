                pwd = vuln.File("/etc/passwd")
                if pwd then
                    if pwd.has_permission("r") then
                        print(pwd.get_content)
                    end if
                end if
                
                users = vuln.File("/home").get_folders

                if users then

                    for user in users

                        bank = vuln.File("/home/"+user.name+"/Config/Bank.txt")
