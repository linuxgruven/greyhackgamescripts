ipgen = function()

    r = range(255,1)
    r.shuffle
    ip = ""

    for i in range(1,4)

        pull = r.pull
        ip = ip + pull+"."

    end for
    ip = ip[:-1]

    if is_lan_ip(ip) then
        ipgen
    end if
    if not is_valid_ip(ip) then
        ipgen
    end if

    //print(ip)
    return ip
end function

