ipgen = function()

    r = range(255,1)
    r.shuffle
    ip = ""

    for i in range(1,4)

        pull = r.pull
        ip = ip + pull+"."

        if i == 1 and pull == "192" then
            ipgen
        end if

    end for
    ip = ip[:-1]

    if not is_valid_ip(ip) then
        ipgen
    end if
    if not get_shell.ping(ip) then
        ipgen
    end if

    return ip
end function


findService = function(service)

    while true
        ip = ipgen
        r = get_router(ip)

        if typeof(r) == "router" then

                ports = r.used_ports

                for port in ports

                    if not r.port_info(port).indexOf(service) == null then
                        return ip
                    end if

                end for

        
        end if


    end while
end function



