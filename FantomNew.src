findService = function(service)

    while true
        ip = ipgen
        r = get_router(ip)

        if typeof(r) == "router" then

                ports = r.used_ports

                for port in ports

                    if not r.port_info(port).indexOf(service) == null then
                        return ip
                    end if

                    split = service.split(" ")
                    if split[0] == "kernel_router" then
                        if r.kernel_version == split[1] then
                            return ip
                        end if
                    end if

                end for

        
        end if


    end while
end function


