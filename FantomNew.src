                unsafe = line[line.indexOf("<b>"):line.indexOf("</b>")]
                //print(unsafe + address)
                unsafe = unsafe.replace("<b>","")
                unsafe = unsafe.replace("</b>","")
                got = lib.overflow(address,unsafe)
                vulns.push([address,unsafe])
            end if
        end for

    end for
    return vulns
end function




